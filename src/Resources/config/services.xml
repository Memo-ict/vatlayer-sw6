<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Memo\Vatlayer\Services\VatlayerService">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
        </service>

        <service id="Memo\Vatlayer\Controllers\VatlayerController" public="true">
            <argument type="service" id="Memo\Vatlayer\Services\VatlayerService" />
        </service>

        <service id="Memo\Vatlayer\Resources\snippet\de_DE\SnippetFile_de_DE" public="true">
            <tag name="shopware.snippet.file"/>
        </service>
        <service id="Memo\Vatlayer\Resources\snippet\en_GB\SnippetFile_en_GB" public="true">
            <tag name="shopware.snippet.file"/>
        </service>
        <service id="Memo\Vatlayer\Resources\snippet\nl_NL\SnippetFile_nl_NL" public="true">
            <tag name="shopware.snippet.file"/>
        </service>
    </services>
</container>
